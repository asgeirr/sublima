<?xml version="1.0"?>
<page  xmlns:jx="http://apache.org/cocoon/templates/jx/1.0" xmlns="http://xmlns.computas.com/cocoon">
  <mode><jx:out value="#{mode}"/></mode>
  <result-list><jx:out value="#{result-list}" xmlize="true"/></result-list>

  <freetext-result-list><!--jx:out value="#{freetext-result-list}" xmlize="true" xmlns:jx="http://apache.org/cocoon/templates/jx/1.0"/--></freetext-result-list>

  <navigation><jx:out value="#{navigation}" xmlize="true"/></navigation>

  <advancedsearch></advancedsearch>

  <facets>
    <jx:forEach var="p" select="${cocoon.request.getParameterNames()}">
      <param key="${p}">
	<jx:forEach var="v" select="${cocoon.request.getParameterValues(p)}">
	  <value>${v}</value>
	</jx:forEach>
      </param>
    </jx:forEach>
  </facets>
</page>