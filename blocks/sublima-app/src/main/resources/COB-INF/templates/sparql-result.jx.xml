<?xml version="1.0" encoding="UTF-8"?>
<page  xmlns:jx="http://apache.org/cocoon/templates/jx/1.0" xmlns="http://xmlns.computas.com/cocoon" xmlns:lingvoj="http://www.lingvoj.org/ontology#"
        xmlns:wdr="http://www.w3.org/2007/05/powder#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:c="http://xmlns.computas.com/cocoon"
        xmlns:skos="http://www.w3.org/2004/02/skos/core#"
        xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:dct="http://purl.org/dc/terms/"
        xmlns:sub="http://xmlns.computas.com/sublima#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:foaf="http://xmlns.com/foaf/0.1/"
        xmlns:i18n="http://apache.org/cocoon/i18n/2.1"
        xmlns:sparql="http://www.w3.org/2005/sparql-results#">
  <mode><jx:out value="#{mode}"/></mode>
  <abovemaxnumberofhits><jx:out value="#{abovemaxnumberofhits}" xmlize="false"/></abovemaxnumberofhits>
  <loggedin><jx:out value="#{loggedin}" xmlize="false"/></loggedin>
  <searchparams><jx:out value="#{searchparams}" xmlize="true"/></searchparams>
  <result-list><jx:out value="#{result-list}" xmlize="true"/></result-list>
  <content>
    <comment><jx:out value="#{comment}" xmlize="true"/></comment>
    <messages>
      <jx:out value="#{messages}" xmlize="true"/>
    </messages>
  </content>
  <navigation><jx:out value="#{navigation}" xmlize="true"/></navigation>

  <facets>
  <jx:out value="#{request}" xmlize="true"/>
  </facets>
</page>