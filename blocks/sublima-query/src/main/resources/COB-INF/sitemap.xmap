<?xml version="1.0" encoding="UTF-8"?>
<map:sitemap xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://apache.org/cocoon/sitemap/1.0 http://cocoon.apache.org/schema/sitemap/cocoon-sitemap-1.0.xsd"
 xmlns:map="http://apache.org/cocoon/sitemap/1.0">

  <map:flow language="spring-apples"/>

  <map:pipelines>  
    <!-- map:pipeline>
      <map:match pattern="sparql">
        <map:call function="com.computas.sublima.query.controller.SparqlQuery"/>
      </map:match>
    </map:pipeline -->
    
    <map:pipeline internal-only="true">
      <map:match pattern="rdf/*">
        <map:generate src="templates/{1}.jx.xml" type="jx"/>
        <map:serialize type="rdf"/>
      </map:match>      
    </map:pipeline>
  </map:pipelines>

</map:sitemap>